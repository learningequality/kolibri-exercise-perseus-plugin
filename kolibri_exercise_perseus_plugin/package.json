    "jquery": "3.4.0",
    "keen-ui": "https://github.com/learningequality/Keen-UI/",
    "kmath": "^0.0.1",
    "performance-now": "^2.1.0",
    "qtip2": "2.2.0",
    "raphael": "^2.2.7",
    "react": "15.3.2",
    "react-addons-create-fragment": "^15.6.0",
    "react-addons-css-transition-group": "15.3.2",
    "react-addons-pure-render-mixin": "^15.6.0",
