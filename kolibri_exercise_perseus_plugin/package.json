{
  "scripts": {
    "postinstall": "git submodule update --init --recursive && cd node_modules/perseus && yarn install",
    "extract-messages": "node ./extractPerseusMessages.js",
    "get-ka-translations": "node ./generateKATranslations.js",
    "download-translations": "node ./downloadTranslations.js",
    "build": "kolibri-tools build prod --plugins kolibri_exercise_perseus_plugin --pluginPath ../",
    "clean": "kolibri-tools build clean --plugins kolibri_exercise_perseus_plugin --pluginPath ../",
    "dev": "kolibri-tools build dev --plugins kolibri_exercise_perseus_plugin --pluginPath ../ --port 4000"
  },
